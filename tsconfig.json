{
	"compilerOptions": {
		"outDir": "./dist/",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"target": "ES2022",
		"module": "ES2022",
		"jsx": "react",
		"allowJs": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"ES2022",
			"dom"
		]
	},
	"exclude": [
		"spec"
	]
}
